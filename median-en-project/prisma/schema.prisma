// MEDIAN EN - Prisma Schema
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String    @id @default(cuid())
  email         String    @unique
  role          String
  walletAddress String?   @unique
  patientDID    String?   @unique
  createdAt     DateTime  @default(now())
}
